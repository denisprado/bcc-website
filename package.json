{
  "name": "bcc-site",
  "description": "BCC Website",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "ssr": "npm run build && npm run serve",
    "format": "prettier --config ./.prettierrc --write src/**/*.js",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "npm run build:dir && npm run build:favicons && gatsby build",
    "test": "ava **/*.test.js --verbose",
    "lint:js": "eslint --fix \"**/*.{js,jsx}\"",
      "lint": "run-p lint:*",
      "precommit": "lint-staged",
      "test-write": "jest --watch",
      "test:lint": "npm run lint",
      "test:functional": "jest"
  },
  "dependencies": {
    "@fullpage/react-fullpage": "^0.1.17",
    "@reach/dialog": "^0.10.0",
    "@reach/router": "^1.2.1",
    "@reach/visually-hidden": "^0.10.0",
    "add": "^2.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react-hooks": "^3.0.0",
    "gatsby": "^2.20.12",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-netlify": "^2.2.1",
    "gatsby-plugin-netlify-cms": "^4.2.2",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-sitemap": "^2.3.1",
    "gatsby-plugin-smoothscroll": "^1.1.0",
    "gatsby-plugin-styled-components": "^3.2.1",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-plugin-webpack-size": "^1.0.0",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-json": "^2.3.1",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.4",
    "intersection-observer": "^0.7.0",
    "locomotive-scroll": "^3.3.11",
    "netlify-cms-app": "^2.12.3",
    "prettier": "^2.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-burger-menu": "^2.6.13",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.9.0",
    "react-pose": "^4.0.5",
    "react-reveal": "^1.2.2",
    "react-scroll-horizontal": "^1.6.2",
    "react-scroll-wheel-handler": "^1.1.3",
    "react-spring": "^8.0.27",
    "styled-components": "^5.0.1",
    "surge": "^0.21.3",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "ava": "^3.6.0",
    "babel-eslint": "^10.0.1",
    "chrome-launcher": "^0.13.1",
    "core-js": "^3.6.4",
    "directory-named-webpack-plugin": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "favicons": "^6.0.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "lighthouse": "^5.6.0",
    "mkdirp": "^1.0.4",
    "node-gyp": "^6.1.0"
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  }
}
